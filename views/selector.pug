extends layout

block content
  h1= title

  - var image_names = ["iphone 12.png","iphone 12pro.jpg","iphone 13.jpg","iphone 14.jpg","iphone 14pro.jpg"];
  - for(i=0;i<3;i++) {
     - var randNum = Math.floor(Math.random()*5) ;
     img(src=`./images/${image_names[randNum]}` alt=`img${image_names[randNum]}` style='width:300px; height:300px;')

  -}